<img src="/images/spceSub.png" id="imagespaceSub">
<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <h1 id="userProfile">My orders History</h1>
        <hr>
        {{# each orders }}
            <div class="panel panel-default">
                <div class="panel-body">
                    <ul class="list-group">
                        {{# each this.items }}
                            <li class="list-group-item">
                                <span class="badge" id="qtyPrice">{{ this.price }}</span>
                                <a id="titleItem">{{ this.item.title }}</a> | <a id="titleItem">{{ this.qty }}Books</a> 
                            </li>
                        {{/each}}
                    </ul>
                </div>
                <div class="panel-footer">
                    <strong>Total Price: ${{ this.cart.totalPrice }}</strong>
                </div>
            </div>
        {{/each}}
    </div>
</div>